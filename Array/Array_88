# Merge sorted array

class Solution:
    def merge(self, nums1: List[int], m: int, nums2: List[int], n: int) -> None:

        i,j=0,0
        while j<n and i<len(nums1):
            if nums1[i]<nums2[j]:
                i+=1
            elif nums2[j]<=nums1[i]:
                nums1.insert(i,nums2[j])
                j+=1
        for _ in range(n):
            nums1.pop()
        while j<n:
            nums1.append(nums2[j])
            j+=1      